<Window x:Class="MyMediaPlayer.LogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LogIn" Height="162" Width="314">
    <Grid>
        <Label Content="User Name:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="18,47,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbLogIn" HorizontalAlignment="Left" Height="23" Margin="86,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="190" />
        <TextBox x:Name="tbLogInPass" HorizontalAlignment="Left" Height="23" Margin="86,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="190"/>
        <Button Content="Log In" HorizontalAlignment="Left" Margin="123,86,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
